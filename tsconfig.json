{
  "include": ["src/**/*"],
  "compilerOptions": {
    "target": "ES6",
    "strict": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": ["jest", "node", "react", "react-dom", "react-test-renderer"],
    "forceConsistentCasingInFileNames": true
  },
}
  